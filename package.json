    "ladda": "^1.0.6",
    "lodash-es": "^4.17.5",
    "moment": "^2.20.1",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.16.0",
    "@types/ladda": "^0.9.30",
    "@types/lodash-es": "^4.17.0",
    "@vue/cli-plugin-pwa": "^3.0.0-alpha.10",
    "@vue/cli-plugin-typescript": "^3.0.0-alpha.10",
